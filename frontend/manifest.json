{
    "version": "1.0.0",
    "manifest_version": 3,
    "permissions": ["activeTab", "tabs"],
    "name": "SkipAI",
    "description": "Parse Lectures Quickly.",
    "action": {
        "default_popup": "dist/js/index.html",
        "default_title": "React Chrome Extension"
    },
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["content.js"]
        }
    ]
}